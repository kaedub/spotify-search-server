{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "properties": {
      "artist": {
        "$id": "#/properties/artist",
        "type": "string",
        "title": "The Artist Schema",
        "default": "",
        "examples": [
          "Amon Tobin"
        ],
        "pattern": "^(.*)$"
      },
      "album": {
        "$id": "#/properties/album",
        "type": "string",
        "title": "The Album Schema",
        "default": "",
        "examples": [
          "Bricolage"
        ],
        "pattern": "^(.*)$"
      },
      "track": {
        "$id": "#/properties/track",
        "type": "string",
        "title": "The Track Schema",
        "default": "",
        "examples": [
          "Easy Muffin"
        ],
        "pattern": "^(.*)$"
      }
    }
  }